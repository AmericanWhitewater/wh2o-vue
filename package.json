{
  "name": "wh2o-vue",
  "version": "0.1.0",
  "private": true,
  "author": "Drew Althage & American Whitewater",
  "scripts": {
    "start": "node server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:production": "sh -ac '. .env.production; vue-cli-service build'",
    "build:qa": "sh -ac '. .env.qa; vue-cli-service build'",
    "build:staging": "sh -ac '. .env.staging; vue-cli-service build'",
    "postinstall": "if test \"$NODE_ENV\" = \"production\" ; then npm run build ; fi ",
    "start": "node server.js"
  },
  "dependencies": {
    "@carbon/grid": "^10.6.0",
    "@carbon/icons-vue": "^10.6.1",
    "@carbon/vue": "^2.15.0",
    "axios": "^0.18.1",
    "chart.js": "^2.9.1",
    "core-js": "^2.6.10",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "register-service-worker": "^1.6.2",
    "tiptap": "^1.26.4",
    "tiptap-extensions": "^1.28.4",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-pwa": "^4.0.5",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "express": "^4.17.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "serve-static": "^1.14.1",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": "10.x"
  },
  "homepage": "https://github.com/drewalth/wh2o-vue"
